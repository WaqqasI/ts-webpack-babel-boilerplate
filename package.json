{
  "name": "react-webpack-typescript-babel",
  "version": "1.0.0",
  "description": "A sample setup using React, Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "prestart": "webpack",
    "start": "node server.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "webpack --progress --colors --mode=production"
  },
  "author": {
    "name": "Alexander T.",
    "email": "alexander.tarasyuk@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "babel-loader": "8.0.6",
    "fork-ts-checker-webpack-plugin": "1.3.3",
    "html-webpack-plugin": "3.2.0",
    "typescript": "3.4.5",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1"
  },
  "dependencies": {
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "express": "^4.17.3",
    "file-loader": "^3.0.1",
    "css-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "html-loader": "^0.5.5",
    "lodash": "^4.17.11",
    "@types/lodash": "^4.14.119"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}